#include<stdio.h>
#include<conio.h>


int main ()
{
    int n , i;
    printf("enter the number of element you want in series:\n");
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    {
        
        printf ("%d \n",i);
    }
    getch();
}

/**
 * case 1:-
 * i = 0;
 * case 2 :-
 * i = 1;
 * i++
 * i = i+1;0+1
 * case3:-
 * i++
 * i=1+1;
 * i=2;
 * 
 * 
*/
